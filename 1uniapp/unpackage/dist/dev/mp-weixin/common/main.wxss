/* ==================
        初始化
 ==================== */
page {
	background-color: #f1f1f1;
	font-size: 28rpx;
	color: #333333;
	/*font-family: Helvetica Neue, Helvetica, sans-serif;*/
}
view,
scroll-view,
swiper,
button,
input,
textarea,
label,
