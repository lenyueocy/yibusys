/* loading */
.thorui-loading {
	position: relative;
	width: 60rpx;
	height: 60rpx;
	display: inline-block;
	vertical-align: middle;
}
.thorui-loading-line {
	width: 8rpx;
	position: absolute;
	border-top-left-radius: 8rpx;
