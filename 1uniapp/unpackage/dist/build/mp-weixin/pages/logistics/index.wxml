<view class="container"><view class="tui-order-header"><view class="tui-text">订单编号：<text class="tui-bold">{{data.ordersn}}</text></view><view class="tui-text">快递公司：<text>{{data.expresscom}}</text></view><view class="tui-text">快递单号：<text>{{data.expresssn}}</text></view></view><view class="tui-order-tracking"><block wx:if="{{expressData}}"><view><tui-time-axis vue-id="309cf339-1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{expressData.Traces}}" wx:for-item="item" wx:for-index="index" wx:key="index"><tui-timeaxis-item vue-id="{{('309cf339-2-'+index)+','+('309cf339-1')}}" backgroundColor="transparent" bind:__l="__l" vue-slots="{{['node','content']}}"><view class="tui-node" style="{{'background-color:'+(index==0?backgroundColor:'')+';'}}" slot="node"><tui-icon vue-id="{{('309cf339-3-'+index)+','+('309cf339-2-'+index)}}" name="{{index==0&&expressData.State==3?'check':'transport'}}" color="#fff" size="{{13}}" bind:__l="__l"></tui-icon></view><view slot="content"><block wx:if="{{index==0&&expressData.State==3}}"><view class="{{['tui-order-title',index==0?'text-black':'text-gray']}}">已签收</view></block><block wx:else><view class="{{['tui-order-title',index==0?'text-black':'text-gray']}}">运输中</view></block><view class="tui-order-desc tui-light-gray">{{item.AcceptStation}}</view><view class="tui-order-time text-gray">{{item.AcceptTime}}</view></view></tui-timeaxis-item></block></tui-time-axis></view></block><block wx:else><view class="text-center text-grey">暂无物流信息</view></block></view></view>