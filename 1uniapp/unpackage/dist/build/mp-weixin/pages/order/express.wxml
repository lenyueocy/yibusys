<view class="container"><view class="tui-order-header"><view class="tui-text">快递公司：<text>{{res.com||'暂无'}}</text></view><view class="tui-text">快递单号：<text>{{res.sn||'暂无'}}</text></view></view><view class="tui-order-tracking"><block wx:if="{{expresslist}}"><view><tui-time-axis vue-id="f2370180-1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{expresslist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><tui-timeaxis-item vue-id="{{('f2370180-2-'+index)+','+('f2370180-1')}}" backgroundColor="transparent" bind:__l="__l" vue-slots="{{['node','content']}}"><view class="tui-node" style="{{'background-color:'+(index==0?backgroundColor:'')+';'}}" slot="node"><tui-icon vue-id="{{('f2370180-3-'+index)+','+('f2370180-2-'+index)}}" name="{{index==0&&expressData.State==3?'check':'transport'}}" color="#fff" size="{{13}}" bind:__l="__l"></tui-icon></view><view slot="content"><block wx:if="{{index==0&&expressData.State==3}}"><view class="{{['tui-order-title',index==0?'text-black':'text-gray']}}">{{res.status+"}"}}</view></block><block wx:else><view class="{{['tui-order-title',index==0?'text-black':'text-gray']}}">运输中</view></block><view class="tui-order-desc tui-light-gray">{{item.step}}</view><view class="tui-order-time text-gray">{{item.time}}</view></view></tui-timeaxis-item></block></tui-time-axis></view></block><block wx:else><view class="text-center text-grey">暂无物流信息</view></block></view></view>